{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:next dev:css",
    "dev:next": "next dev",
    "dev:css": "tcm ./src --watch",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint ./src",
    "format": "biome format ./src --write",
    "check": "biome check ./src",
    "test": "vitest",
    "test:update": "vitest -u",
    "api:build": "aspida -c ./aspida.config.js && aspida-mock"
  },
  "dependencies": {
    "@aspida/fetch": "^1.14.0",
    "aspida": "^1.14.0",
    "aspida-mock": "^0.11.1",
    "clsx": "^2.1.1",
    "next": "15.1.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
